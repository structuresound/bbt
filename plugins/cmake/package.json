{
  "name": "tmake-plugin-cmake",
  "description": "handle CMakeLists.txt with tmake",
  "version": "0.6.0",
  "homepage": "https://github.com/structuresound/tmake",
  "author": {
    "name": "Leif Shackelford",
    "email": "structuresound@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/structuresound/tmake.git"
  },
  "bugs": {
    "url": "https://github.com/structuresound/tmake/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/structuresound/tmake/blob/master/LICENSE-MIT"
    }
  ],
  "main": "dist/index.js",
  "types": "src/cmake.d.ts",
  "scripts": {
    "build": "gulp scripts",
    "test": "npm run build && NODE_ENV=test ../../node_modules/.bin/mocha --recursive --compilers ts:ts-node/register --bail",
    "debug": "NODE_ENV=test ../../node_modules/.bin/mocha --recursive --compilers ts:ts-node/register --bail --inspect=PORT --debug-brk",
    "cover": "NODE_ENV=test ../../node_modules/.bin/nyc --report-dir coverage npm t",
    "travis": "npm run build && npm run cover"
  },
  "peerDependencies": {
    "tmake-core": "0.6.1"
  },
  "Keywords": [],
  "dependencies": {},
  "devDependencies": {
    "tmake-file": "^0.6.6"
  }
}
